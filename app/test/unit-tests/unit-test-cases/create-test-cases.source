#!/bin/bash

script_name="$(basename ${BASH_SOURCE})"
cur_test_num="0"

(( ++cur_test_num ))
(tsi_create > "/dev/null")
tsi_handle_test_exit_status \
    "${invalid_command_args_count_err_exit_status}" \
    "${?}" \
    "${cur_test_num}" \
    "${script_name}"

(( ++cur_test_num ))
(tsi_create "" > "/dev/null")
tsi_handle_test_exit_status \
    "${empty_command_err_exit_status}" \
    "${?}" \
    "${cur_test_num}" \
    "${script_name}"

(( ++cur_test_num ))
(tsi_create "abc" > "/dev/null")
tsi_handle_test_exit_status \
    "${unrecognized_command_err_exit_status}" \
    "${?}" \
    "${cur_test_num}" \
    "${script_name}"